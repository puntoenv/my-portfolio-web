---
import ExperienceItem from "@components/ExperienceItem.astro";
import EXPERIENCE from "@data/experience.json";
---

<ol
  class="relative border-s border-gray-200 dark:border-gray-700 space-y-8 mt-4 pl-4"
>
  {
    EXPERIENCE.map(({ title, latest, time, description }) => (
      <ExperienceItem
        title={title}
        latest={latest}
        time={time}
        description={description}
      />
    ))
  }
</ol>
